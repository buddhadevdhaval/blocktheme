{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ambrygen/gallery-item",
  "title": "Gallery Item",
  "parent": [
    "ambrygen/gallery"
  ],
  "supports": {
    "html": false
  },
  "attributes": {
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "imageAlt": {
      "type": "string",
      "default": ""
    },
    "imageSrcSet": {
      "type": "string",
      "default": ""
    },
    "imageSizes": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "headingTag": {
      "type": "string",
      "default": "h5"
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "link": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}