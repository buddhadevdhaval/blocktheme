			"blockGap": true
		},
		"layout": {
			"allowSwitching": true,
			"allowInheriting": true,
			"allowEditing": true
		},
		"color": {
			"text": true,
			"background": true,
			"link": true
		},
		"typography": true
	},
	"textdomain": "ambrygen-web",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}