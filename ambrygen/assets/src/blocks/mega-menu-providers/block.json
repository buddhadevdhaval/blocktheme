{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ambrygen/mega-menu-providers",
	"version": "1.0.0",
	"title": "Mega Menu Providers",
	"category": "ambrygen",
	"icon": "menu",
	"description": "Providers Mega Menu Block",
	"supports": {
		"html": false,
		"align": [
			"full",
			"wide"
