{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "ambrygen/testimonials",
    "title": "Testimonials Section",
    "category": "ambrygen",
    "icon": "format-quote",
    "supports": {
        "color": {
            "background": true
        }
    },
    "attributes": {
        "heading": {
            "type": "string",
            "default": "Read About Ambryâ€™s Impact on Patient Lives"
        },
        "headingTag": {
            "type": "string",
            "default": "h2"
        },
        "mainImage": {
            "type": "string",
            "default": "/wp-content/themes/ambrygen/assets/src/images/testimonial/testimonial-main.png"
        },
        "mainImageSizes": {
            "type": "object",
            "default": {}
        },
        "secondaryImage": {
            "type": "string",
            "default": "/wp-content/themes/ambrygen/assets/src/images/testimonial/secondary-image.png"
        },
        "secondaryImageSizes": {
            "type": "object",
            "default": {}
        },
        "overlayImage": {
            "type": "string",
            "default": "/wp-content/themes/ambrygen/assets/src/images/testimonial/overlay-image.png"
        },
        "overlayImageSizes": {
            "type": "object",
            "default": {}
        }
    },
    "editorScript": "file:./index.js",
    "render": "file:./render.php",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "textdomain": "ambrygen-web",
    "providesContext": {
		"ambrygen/mainImage": "mainImage",
		"ambrygen/mainImageSizes": "mainImageSizes"
	}

}