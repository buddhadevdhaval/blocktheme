					"background": [],
					"customDuotone": [],
					"customGradient": [],
					"defaultDuotone": [],
					"defaultGradients": [],
					"duotone": [],
					"gradients": []
				},
				"typography": {
					"customFontSize": [],
					"fontSizes": []
				},
				"border": {
					"radius": []
				}
			},
			"core/navigation": {
				"color": {
					"text": ["black"]
				}
			},
			"core/paragraph": {
				"color": {
					"text": ["black"]
				}
			}
		}
	}
}
