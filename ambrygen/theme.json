				{ "name": "2 Pixels", "size": "var(--gb_space_1)", "slug": "2-pixels", "default": true },
				{ "name": "4 Pixels", "size": "var(--gb_space_2)", "slug": "4-pixels" },
				{ "name": "6 Pixels", "size": "var(--gb_space_3)", "slug": "6-pixels" },
				{ "name": "8 Pixels", "size": "var(--gb_space_4)", "slug": "8-pixels" },
