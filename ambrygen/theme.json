			"defaultGradients": false,
			"defaultDuotone": false,
			"customGradient": false,
			"customDuotone": false,
			"gradients": [],

			"palette": [
				{
					"slug": "white",
					"color": "var(--theme_white)",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "var(--theme_black1)",
					"name": "Black"
				},
				{
					"slug": "primary",
					"color": "var(--theme_color_primary)",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "var(--theme_color_secondary)",
					"name": "Secondary"
				},
				{
					"slug": "danger",
					"color": "var(--theme_danger)",
					"name": "Danger"
				},
				{
					"slug": "success",
