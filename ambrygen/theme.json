				{ "name": "236 Pixels", "size": "var(--gb_space_17)", "slug": "236-pixels" }
			]
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": [300, 400, 500, 600, 700],
			"letterSpacing": [],
			"lineHeight": [
				{ "slug": "line-height-heading", "size": "var(--line_height_medium)", "name": "Line Height heading" }
			],
			"textDecoration": [],
			"textTransform": [],
			"dropCap": false,
			"fontSizes": [
				{ "slug": "font-size-1", "size": "var(--gb_font_size_1)", "name": "Heading 1" },
				{ "slug": "font-size-2", "size": "var(--gb_font_size_2)", "name": "Heading 2" },
