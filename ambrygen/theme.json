{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"border": {
			"color": [],
			"radius": [],
			"style": [],
			"width": true
		},
		"color": {
			"custom": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"customGradient": false,
			"customDuotone": false,
			"gradients": [],
			"palette": [
				{ "slug": "white", "color": "var(--gb_white)", "name": "White" },
				{ "slug": "black", "color": "var(--gb_black1)", "name": "Black" },
				{ "slug": "primary", "color": "var(--gb_color_primary)", "name": "Primary" },
				{ "slug": "secondary", "color": "var(--gb_color_secondary)", "name": "Secondary" },
				{ "slug": "danger", "color": "var(--gb_danger)", "name": "Danger" },
				{ "slug": "success", "color": "var(--gb_success)", "name": "Success" },
				{ "slug": "warning", "color": "var(--gb_warning)", "name": "Warning" },
				{ "slug": "info", "color": "var(--gb_info)", "name": "Info" },
				{ "slug": "transparent", "color": "var(--gb_transparent)", "name": "Transparent" }
			],
			"text": "var(--gb_black1)",
			"link": "var(--gb_color_primary)"
		},
		"layout": {
			"contentSize": "1180px",
			"wideSize": "1380px"
		},
		"spacing": {
			"margin": true,
			"padding": [],
